{
  "extends": ["next/core-web-vitals","prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-console": "warn",
    "max-lines": ["error", 500],
    "max-lines-per-function": ["error", 150],
    // "semi": ["error", "never"],
    "quotes": ["error", "double"],
    "import/prefer-default-export": "off",
    "import/no-unresolved": "off",
    "no-unused-vars": "off",
    "camelcase": "warn",
    "new-cap": "off"
  }
}